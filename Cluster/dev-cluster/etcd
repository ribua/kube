# etcd ouput

for POD in `oc get po -n openshift-etcd |awk '/etcd-/ {print $1}'|grep -v quorum`; do oc -n openshift-etcd exec -c etcd $POD -- /bin/bash -c "etcdctl endpoint status -w table";done
